{
  "name": "edenos-mcp-bridge",
  "version": "1.0.0",
  "description": "EdenOS MCP Bridge - Comprehensive Model Context Protocol server with GCP, Firebase, and development tools",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && node esbuild.mjs",
    "build:ui": "vite build",
    "serve:ui": "vite preview --strictPort --port 4173",
    "dev:ui": "vite",
    "preview:ui": "vite preview",
    "start": "node dist/server.js",
    "deploy": "npm run build && npm run build:ui && firebase deploy",
    "deploy:hosting": "npm run build:ui && firebase deploy --only hosting",
    "deploy:functions": "npm run build && firebase deploy --only functions",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gcp",
    "firebase",
    "firestore",
    "bigquery",
    "github-actions",
    "cloud-run",
    "terraform",
    "devops"
  ],
  "author": "EdenOS Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/aiplatform": "^5.6.0",
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/logging": "^11.2.0",
    "@google-cloud/pubsub": "^5.2.0",
    "@google-cloud/run": "^3.0.0",
    "@google-cloud/scheduler": "^5.3.0",
    "@google-cloud/storage": "^7.17.0",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@slack/web-api": "^7.10.0",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "telegraf": "^4.16.3",
    "three": "^0.179.1",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.0.0",
    "esbuild": "^0.19.0",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
