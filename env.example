# EdenOS MCP Bridge Environment Variables
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=production
PORT=3000
BRIDGE_ADMIN_TOKEN=your-admin-token-here

# Notion Integration
NOTION_API_KEY=your-notion-api-key

# Linear Integration
LINEAR_API_KEY=your-linear-api-key

# GitHub Integration
GITHUB_TOKEN=your-github-token

# Firebase Integration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_PRIVATE_KEY=your-firebase-private-key
FIREBASE_CLIENT_EMAIL=your-firebase-client-email

# GCP Integration
GCP_PROJECT_ID=your-gcp-project-id
GCP_PRIVATE_KEY=your-gcp-private-key
GCP_CLIENT_EMAIL=your-gcp-client-email

# Figma Integration
FIGMA_ACCESS_TOKEN=your-figma-access-token

# Zapier Integration
ZAPIER_API_KEY=your-zapier-api-key

# Bnd Integration
BND_API_KEY=your-bnd-api-key

# Saviynt Integration
SAVIYNT_API_KEY=your-saviynt-api-key

# Anthropic Integration
ANTHROPIC_API_KEY=your-anthropic-api-key

# Neon Integration
NEON_API_KEY=your-neon-api-key
NEON_PROJECT_ID=your-neon-project-id

# DeepMind Integration
DEEPMIND_API_KEY=your-deepmind-api-key
DEEPMIND_PROJECT_ID=your-deepmind-project-id

# OpenAI Integration
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORGANIZATION=your-openai-organization-id
OPENAI_DEFAULT_MODEL=gpt-4o

# Box Integration
BOX_ACCESS_TOKEN=your-box-access-token
BOX_CLIENT_ID=your-box-client-id
BOX_CLIENT_SECRET=your-box-client-secret

# Fun Features (Optional)
SPOTIFY_CLIENT_ID=your-spotify-client-id
SPOTIFY_CLIENT_SECRET=your-spotify-client-secret
SLACK_BOT_TOKEN=your-slack-bot-token
TELEGRAM_BOT_TOKEN=your-telegram-bot-token